        },
        "AdminNetwork": {
            "Type": "String",
            "Value": "10.255.20.121/24"
        },
        "PublicNetwork": {
            "Type": "String",
            "Value": "10.255.20.121/24"
        },
        "PrivateNetwork": {
            "Type": "String",
            "Value": "10.255.30.121/24"
        },
        "GatewayNetwork": {
